{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions", "not dead"]
      }
    }],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": [
    ["babel-plugin-styled-components", {
      "displayName": true,
      "fileName": false,
      "pure": true,
      "ssr": false,
      "transpileTemplateLiterals": true,
      "minify": true
    }]
  ]
}
